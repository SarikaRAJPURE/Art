t_type:{shape:"primitive",primitive:"string"}},"customer-creation":{type:"param",description:"Configure whether a Checkout Session creates a Customer during Session confirmation.  When a Customer is not created, you can still retrieve email, address, and other customer data entered in Checkout with customer_details.  Sessions that don't create Customers instead are grouped by guest customers in the Dashboard. Promotion codes limited to first time customers will return invalid for these Sessions.  Can only be set in `payment` and `setup` mode.",required:!1,input_type:{shape:"enum",enum:["always","if_required"]}},"customer-email":{type:"param",description:"If provided, this value will be used when the Customer object is created. If not provided, customers will be asked to enter their email address. Use this parameter to prefill customer data if you already have an email on file. To access information about the customer once a session is complete, use the `customer` field.",required:!1,input_type:{shape:"primitive",primitive:"string"}},"customer-update":{type:"param",description:"Controls what fields on Customer can be updated by the Checkout Session. Can only be provided when `customer` is provided.",required:!1,input_type:{shape:"object",fields:[{name:"address",required:!1,type:{shape:"enum",enum:["auto","never"]},description:"Describes whether Checkout saves the billing address onto `customer.address`.\nTo always collect a full billing address, use `billing_address_collection`. Defaults to `never`."},{name:"name",required:!1,type:{shape:"enum",enum:["auto","never"]},description:"Describes whether Checkout saves the name onto `customer.name`. Defaults to `never`."},{name:"shipping",required:!1,type:{shape:"enum",enum:["auto","never"]},description:"Describes whether Checkout saves shipping information onto `customer.shipping`.\nTo collect shipping information, use `shipping_address_collection`. Defaults to `never`."}],title:"customer_update_params",description:"Controls what fields on Customer can be updated by the Checkout Session. Can only be provided when `customer` is provided.",isPolymorphic:!1,polymorphicVariants:[],semantics:"v1"}},discounts:{type:"param",description:"The coupon or promotion code to apply to this Session. Currently, only up to one may be specified.",required:!1,input_type:{shape:"array",elementType:{shape:"object",fields:[{name:"coupon",required:!1,type:{shape:"primitive",primitive:"string"},description:"The ID of the coupon to apply to this Session."},{name:"promotion_code",required:!1,type:{shape:"primitive",primitive:"string"},description:"The ID of a promotion code to apply to this Session."}],title:"discount_params",isPolymorphic:!1,polymorphicVariants:[],semantics:"v1"}}},expand:{type:"param",description:"Specifies which fields in the response should be expanded.",required:!1,input_type:{shape:"array",elementType:{shape:"primitive",primitive:"string"}}},"expires-at":{type:"param",description:"The Epoch time in seconds at which the Checkout Session will expire. It can be anywhere from 30 minutes to 24 hours after Checkout Session creation. By default, this value is 24 hours from creation.",required:!1,input_type:{shape:"primitive",primitive:"DateTime"}},"invoice-creation":{type:"param",description:"Generate a post-purchase Invoice for one-time payments.",required:!1,input_type:{shape:"object",fields:[{name:"enabled",required:!0,type:{shape:"primitive",primitive:"boolean"},description:"Set to `true` to enable invoice creation."},{name:"invoice_data",required:!1,type:{shape:"object",fields:[{name:"account_tax_ids",required:!1,type:{shape:"emptyStringable",type:{shape:"array",elementType:{shape:"primitive",primitive:"string"}}},description:"The account tax IDs associated with the invoice."},{name:"custom_fields",required:!1,type:{shape:"emptyStringable",type:{shape:"array",elementType:{shape:"object",fields:[{name:"name",required:!0,type:{shape:"primitive",primitive:"string"},description:"The name of th